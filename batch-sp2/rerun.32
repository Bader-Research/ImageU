#!/bin/csh

set p    = 32
set mach = "CM5"
set k    = 11

connComp -U 999 -o $mach-hrelS.$p.out
connComp -U 999 -o $mach-hrelS.$p.out
connComp -U 999 -o $mach-hrelS.$p.out
connComp -U 999 -o $mach-hrelS.$p.out
connComp -U 999 -o $mach-hrelS.$p.out

while ($k < 19)
@ k = $k + 1

 connComp -U $k -o $mach-radixS.$p.out
 connComp -U $k -o $mach-radixS.$p.out
 connComp -U $k -o $mach-radixS.$p.out
 connComp -U $k -o $mach-radixS.$p.out

end


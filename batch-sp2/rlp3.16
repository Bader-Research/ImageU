  poe $dir/connComp_v1 -U 9999 -o v1-$mach-lperm.$p.out
  poe $dir/connComp_v1 -U 9999 -o v1-$mach-lperm.$p.out
  poe $dir/connComp_v1 -U 9999 -o v1-$mach-lperm.$p.out

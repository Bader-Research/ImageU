  poe $dir/connComp_v3 -U 9999 -o v3-$mach-lperm.$p.out
  poe $dir/connComp_v3 -U 9999 -o v3-$mach-lperm.$p.out
  poe $dir/connComp_v3 -U 9999 -o v3-$mach-lperm.$p.out
  poe $dir/connComp_v3 -U 9999 -o v3-$mach-lperm.$p.out
  poe $dir/connComp_v3 -U 9999 -o v3-$mach-lperm.$p.out

  poe $dir/connComp_v4 -U 9999 -o v4-$mach-lperm.$p.out
